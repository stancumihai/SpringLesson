<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>addServlet</servlet-name>
        <servlet-class>ro.stancumihai.AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>squareServlet</servlet-name>
        <servlet-class>ro.stancumihai.SquareServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>squareServlet</servlet-name>
        <url-pattern>/square</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>multiplyServlet</servlet-name>
        <servlet-class>ro.stancumihai.MultiplyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>multiplyServlet</servlet-name>
        <url-pattern>/multiply</url-pattern>
    </servlet-mapping>

</web-app>